<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\msmaven\super_pom.props"/>

  <PropertyGroup>
    <mvpackage_dir Condition = " '$(mvpackage_dir)' == '' ">$(MSBuildProjectDirectory)</mvpackage_dir>
    <mvpackage_platform Condition = " '$(mvpackage_platform)' == '' ">Win32</mvpackage_platform>

    <mvpackage_name>xunittest</mvpackage_name>
    <mvpackage_group>com.virtuos.tnt</mvpackage_group>
    <mvpackage_version_major>1</mvpackage_version_major>
    <mvpackage_version_minor>0</mvpackage_version_minor>
    <mvpackage_platforms>Win32;SonyPSP;SonyPS3;NintendoDS;Nintendo3DS;NintendoWII;MicrosoftXbox360</mvpackage_platforms>
    <mvpackage_language>cpp</mvpackage_language>
    <mvpackage_proj>$(mvpackage_name).sln</mvpackage_proj>
    <mvpackage_include_dir>source\main\$(mvpackage_language)\include</mvpackage_include_dir>
    <mvpackage_library_dir>libs\$(mvpackage_name)</mvpackage_library_dir>
    <mvpackage_defines></mvpackage_defines>
  </PropertyGroup>

</Project>